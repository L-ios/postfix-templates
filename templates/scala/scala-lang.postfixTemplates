.println : println
    NON_VOID                        →   println($expr$)

.show : println
    NON_VOID                        →   println($expr$)

.val : val
    NON_VOID                        →   val $name$ = $expr$

.var : var
    NON_VOID                        →   var $name$ = $expr$

.class : class
    ANY                             →   classOf[$expr$]
